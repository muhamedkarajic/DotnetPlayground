{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "WebAPI",
            "type": "coreclr",
            "request": "launch",
            "checkForDevCert": false,
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/WebAPI/bin/Debug/net6.0/osx.12-arm64/WebAPI",
            "cwd": "${workspaceFolder}/WebAPI",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            },
        }
    ]
}